    "    c_weight = trial.suggest_categorical(\"c_weight\", [\"balanced\", None])\n",
    "\n",
    "    clf = LogisticRegression(C=C, class_weight=c_weight)\n",
    "    clf.fit(X_train, y_train)\n",
    "\n",
